<h1> Student Form</h1>

<% if flash[:my_error] %>
    <% my_error.each do |error| %>
    <%= error %>
    <% end %>    
<% end %>
    
<%= form_for @student do |f| %>
<%= f.label :name, "Name" %>
<%= f.text_field :name %>

<%= f.label :mod, "Mod" %>
<%= f.number_field (:mod, in: ${1...5}) %>

<%= submit_tag "Create Student" %>
<% end %>